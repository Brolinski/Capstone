@using Scheduler.Core.Services;

@inject IRepository<Team> TeamService
@model Practice

@{
    // This is slow as a database call is made for each instance.
    this.Model.Team = await this.TeamService.GetAsync(this.Model.TeamId);
}

<p class="card-text">
    <h5>Who:</h5>
    @this.Model.Team.Name
</p>