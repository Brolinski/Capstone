@{
    this.Layout = "_Layout";

    string[] navItems = new[] { "Profile", "Security" };
}

<div class="row w-75 m-auto">
    <aside class="col-sm-2">
        @await this.Component.InvokeAsync(
            "Navigation",
            new { navItems })
    </aside>
    <div class="col-sm-10">
        @this.RenderBody()
    </div>
</div>