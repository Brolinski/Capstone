@model Team

@{
    this.ViewData["Title"] = "Edit Team";
}

<div class="row">
    <div class="col-sm-6 mx-auto">
        <form method="post">
            <h1 class="text-center text-danger">
                @this.ViewData["Title"]
            </h1>
            <hr />

            @await this.Html.PartialAsync("Forms/_TeamInputs", this.Model)
        </form>
    </div>
</div>

<script>
    $(document).ready(function () {

        $("#coachName").chosen({
            no_results_text: "Sorry, I can't find that coach!",
            placeholder_text_single: "Start typing to select a coach...",
            width: "100%",
            search_contains: true
        });
    });
</script>